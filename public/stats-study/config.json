        "consent": {
            "type": "markdown",
            "path": "stats-study/assets/consent.md",
            "style": {
                "width": "800px",
                "margin": "0 auto"
            },
            "nextButtonText": "Continue",
            "response": [
                {
                    "id": "accept",
                    "prompt": "Are you ready to continue?",
                    "requiredValue": "Accept",
                    "location": "belowStimulus",
                    "style": {
                        "width": "800px",
                        "margin": "0 auto"
                    },
                    "type": "radio",
                    "options": [
                        "Decline",
                        "Accept"
                    ]
