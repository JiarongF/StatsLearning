        "autoDownloadStudy": false,
        "urlParticipantIdParam": "PROLIFIC_PID",
        "withSidebar": true,
        "recordAudio": true,
        "recordScreen": true,
        "recordScreenFPS": 30,
        "studyEndMsg": "**Thank you for completing the study. You may click this link and return to Prolific**: [https://app.prolific.com/submissions/complete?cc=CB9Y2414](https://app.prolific.com/submissions/complete?cc=CB9Y2414)"
